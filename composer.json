{
    "name": "demontpx/parsedown-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Parsedown (markdown) bundle for Symfony",
    "keywords": ["markdown", "parsedown", "md", "symfony", "bundle"],
    "autoload": {
        "psr-4": { "Demontpx\\ParsedownBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\ParsedownBundle\\": "tests/" }
    },
    "require": {
        "php": "^7.4",
        "symfony/config": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "twig/twig": "^2.4|^3.0",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "mockery/mockery": "^1.3",
        "symfony/yaml": "^4.4|^5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
